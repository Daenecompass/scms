<p id="notice"><%= notice %></p>

<h1>Listing Sc Events</h1>

<table>
    <thead>
    <tr>
        <th colspan="3"></th>
    </tr>
    </thead>

    <tbody>
    <% @sc_events.each do |sc_event| %>
        <tr>
            <td><%= link_to 'Show', sc_event %></td>
            <td><%= link_to 'Edit', edit_sc_event_path(sc_event) %></td>
            <td><%= link_to 'Destroy', sc_event, method: :delete, data: {confirm: 'Are you sure?' } %></td>
        </tr>
    <% end %>
    </tbody>
</table>

<br>

