<p id="notice"><%= notice %></p>

<h1>Listing Rights</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Holder</th>
      <th>Grantor</th>
      <th>Contract</th>
      <th>Name</th>
      <th>Subsists</th>
    </tr>
  </thead>

  <tbody>
    <% @rights.each do |right| %>
      <tr>
        <td><%= right.holder_user.name %></td>
        <td><%= right.grantor_user.name %></td>
        <td><%= right.contract.id %></td>
        <td><%= right.name %></td>
        <td><%= right.subsists %></td>
      </tr>
    <% end %>
  </tbody>
</table>
