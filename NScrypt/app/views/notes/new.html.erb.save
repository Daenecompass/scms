<h1>New Note</h1>

<%= render 'form' %>

  <div class="control-group">
    <%= f.label :template, :class => 'control-label' %>
    <div class="controls">
      <%= f.select(:code, options_from_collection_for_select(::Template.all, :id, :name)) %>
    </div>
    <%= error_span(@code[:code]) %>
  </div>

<%= link_to 'Back', notes_path %>
