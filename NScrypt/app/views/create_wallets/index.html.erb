<p id="notice"><%= notice %></p>

<h1>Listing Create Wallets</h1>

<table>
  <thead>
    <tr>
      <th>Currency</th>
      <th>Address</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @create_wallets.each do |create_wallet| %>
      <tr>
        <td><%= create_wallet.currency %></td>
        <td><%= create_wallet.address %></td>
        <td><%= create_wallet.user %></td>
        <td><%= link_to 'Show', create_wallet %></td>
        <td><%= link_to 'Edit', edit_create_wallet_path(create_wallet) %></td>
        <td><%= link_to 'Destroy', create_wallet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Create wallet', new_create_wallet_path %>
